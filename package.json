{
  "name": "nextjs-starter-with-typescript",
  "description": "A TypeScript starter for Next.js that includes all you need to start building projects",
  "version": "1.0.0",
  "private": true,
  "author": "Johnny Carreiro <johnny@johnnycarreiro.com> (johnnycarreiro.com)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript",
    "template"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@prismicio/client": "^5.1.0",
    "@types/prismic-dom": "^2.1.1",
    "@unform/core": "^2.1.6",
    "@unform/web": "^2.1.6",
    "axios": "^0.21.4",
    "lodash.throttle": "^4.1.1",
    "next": "^11.1.2",
    "next-images": "^1.8.1",
    "nodemailer": "^6.7.0",
    "nookies": "^2.5.2",
    "prismic-dom": "^2.2.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-spring": "^8.0.27",
    "styled-components": "^5.3.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@svgr/webpack": "^5.5.0",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^16.9.1",
    "@types/nodemailer": "^6.4.4",
    "@types/nookies": "^2.0.3",
    "@types/react-dom": "^17.0.10",
    "@types/react-icons": "^3.0.0",
    "@types/styled-components": "^5.1.14",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "7.24.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "^4.4.3"
  }
}
